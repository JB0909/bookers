<h1>Books</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Body</th>
        </tr>
    </thead>
    <tbody>
        <% @books.each do |book| %>
            <tr>
                <td><%= book.title %></td>
                <td><%= book.body %></td>
                <td><%= link_to "show", book %></td>
                <td><%= link_to "edit", edit_book_path(book) %></td>
                <td><%= link_to "destroy", book_path(book), method: :delete %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<h1>New book</h1>
<%= render 'books/new' %>
